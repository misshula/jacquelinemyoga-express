body
  padding: 50px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif a
  color: #00B7FF

#header
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	z-index: 90000;
	font-family: "News Gothic MT W01 Bold",Arial,Helvetica,sans-serif;
	text-transform: uppercase;
	line-height: 12px;
	letter-spacing: 0.2em;
	background: #fff;

#header_padding
	border-bottom: 1px solid #2d2b2d;
	margin: 0 35px;
	padding-top: 20px;
	padding-bottom: 20px;

#nav_about
#nav_contact
#nav_schedule
#nav_heart
	padding-right: 20px


#home
#contact
	padding: -50px;
	margin: 0px;
	width: 100%;
	height: 100%;
	position: absolute;

#footer
	position: fixed;
	width: 100%;
	background: #fff;
	bottom: 0;
	right: 0;
	text-align: right;
	height: 128px;
	line-height: 12px;
	z-index: 4000;

#footer_links
	z-index: 99000;
	position: fixed;
	bottom: 53px;
	right: 35px;
	line-height: 18px;
	height: 18px;
	width: 100%;

#footer_links a
	margin: 0 0 0 20px;
	padding: 0 0 0 20px;
	border-left: 1px solid #2d2b2d;
	float: right;