body
	font-family: 'Enriqueta', serif;
	font-size: 11px;
	color: black

#header
	position: fixed;
	top: 0;
	left: 0;
	width: 100%
	z-index: 90000;
	font-family: 'Raleway', sans-serif;
	font-size: 14px;
	text-transform: uppercase;
	line-height: 12px;
	letter-spacing: 0.2em;
	background: rgb(243, 243, 243);
	border-bottom: 1px solid #2d2b2d;

#header_padding
	margin: 0 35px;
	padding-top: 20px;
	padding-bottom: 20px;

#nav_about
#nav_contact
#nav_schedule
#nav_heart
#jacquelinemyoga a
#nav_linkedin
#nav_twitter
#nav_facebook
	padding-right: 20px;
	text-decoration: none;
	color: black;

#home
#about
#contact
#schedule
#heart
	margin-left: -8px;
	width: 100%;
	height: 100%;
	position: absolute;
	display: none;
	transition: opacity 2s linear;

.modal
	border: 1px black;
	display: block;
	z-index: 1000000;
	background: white;
	opacity: 0.8;
	border-radius: 10px;
	box-shadow: 2px 2px 2px #888;
	padding: 4px;

#homemodal
	width: 371px;
	height: 101px;
	margin-left: auto;
	margin-right: auto;
	margin-bottom: 100px;
	position: absolute;
	bottom: 0;
	left: 0;
	right: 0;
	display: none;

#homemodal p
	margin-top: 5px;

#homemodal p.indent
	margin-left: 30px;

#homemodal p.end
	margin-left: 220px;

#footer
	position: fixed;
	width: 100%;
	background: rgb(243, 243, 243);;
	bottom: 0;
	right: 0;
	text-align: right;
	height: 85px;
	line-height: 12px;
	z-index: 4000;
	border-top: 1px solid #2d2b2d;

#jacquelinemyoga
	font-family: 'Raleway', sans-serif;
	font-size: 45px;
	text-align: left;
	padding-left: 10%;
	padding-top: 3.5%;

#footer_links
	z-index: 99000;
	position: fixed;
	bottom: 55px;
	right: 35px;
	line-height: 18px;
	height: 18px;
	width: 100%;

#footer_links a
	float: right;

#nav_facebook a
	border-left: none;

.circle
	width: 64px;
	height: 64px;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
	-moz-background-clip: padding;
	-webkit-background-clip: padding-box;
	background-clip: padding-box;
	background: #E0371E;
	display: inline-block;
	-webkit-box-shadow: inset 0 1px 2px #AF2C19;
	-moz-box-shadow: inset 0 1px 2px #af2c19;
	box-shadow: inset 0 1px 2px #AF2C19;
	-webkit-transition: all 0.2s linear;
	-moz-transition: all 0.2s linear;
	-ms-transition: all 0.2s linear;
	-o-transition: all 0.2s linear;
	transition: all 0.2s linear;
	margin: 0 12px;
	text-align: center;

.circle:hover
	background:#d5270d;
	-webkit-transform:translate3d(0, -5px, 0);
	-moz-transform:translatey(-5px);
	-ms-transform:translatey(-5px);
	-o-transform:translatey(-5px);
	transform:translatey(-5px);

.circle span
	background: url("/images/icons.png") -174px 0 no-repeat;
	width: 40px;
	height: 40px;
	margin-top: 13px;
	display: inline-block;

.circle.twitter
	background: #00ACE9;
	-webkit-box-shadow: inset 0 1px 2px #1E7DA4;
	-moz-box-shadow: inset 0 1px 2px #1e7da4;
	box-shadow: inset 0 1px 2px #1E7DA4;

.circle.twitter span
	background: url("/images/icons.png") -214px 0 no-repeat;

.circle.facebook
	background: #3B5998;
	-webkit-box-shadow: inset 0 1px 2px #1E7DA4;
	-moz-box-shadow: inset 0 1px 2px #1e7da4;
	box-shadow: inset 0 1px 2px #1E7DA4;
	color: white;
	font-size: 38px;

.circle.facebook span 
	background: none;
	margin-top: 23px;