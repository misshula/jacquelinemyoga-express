.circle
	width: 64px;
	height: 64px;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
	-moz-background-clip: padding;
	-webkit-background-clip: padding-box;
	background-clip: padding-box;
	background: #E0371E;
	display: inline-block;
	-webkit-box-shadow: inset 0 1px 2px #AF2C19;
	-moz-box-shadow: inset 0 1px 2px #af2c19;
	box-shadow: inset 0 1px 2px #AF2C19;
	-webkit-transition: all 0.2s linear;
	-moz-transition: all 0.2s linear;
	-ms-transition: all 0.2s linear;
	-o-transition: all 0.2s linear;
	transition: all 0.2s linear;
	margin: 0 12px;
	text-align: center;

.circle:hover
	background:#d5270d;
	-webkit-transform:translate3d(0, -5px, 0);
	-moz-transform:translatey(-5px);
	-ms-transform:translatey(-5px);
	-o-transform:translatey(-5px);
	transform:translatey(-5px);

.circle span
	background: url("/images/icons.png") -174px 0 no-repeat;
	width: 40px;
	height: 40px;
	margin-top: 13px;
	display: inline-block;

.circle.twitter
	background: #00ACE9;
	-webkit-box-shadow: inset 0 1px 2px #1E7DA4;
	-moz-box-shadow: inset 0 1px 2px #1e7da4;
	box-shadow: inset 0 1px 2px #1E7DA4;

.circle.twitter span
	background: url("/images/icons.png") -214px 0 no-repeat;

.circle.facebook
	background: #3B5998;
	-webkit-box-shadow: inset 0 1px 2px #1E7DA4;
	-moz-box-shadow: inset 0 1px 2px #1e7da4;
	box-shadow: inset 0 1px 2px #1E7DA4;
	color: white;
	font-size: 38px;

.circle.facebook span 
	background: none;
	margin-top: 23px;